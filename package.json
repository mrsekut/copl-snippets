{
  "name": "copl-snippets",
  "version": "0.0.2",
  "publisher": "mrsekut",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrsekut/copl-snippets"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  },
  "contributes": {
    "languages": [
      {
        "id": "copl",
        "extensions": [
          ".copl"
        ]
      }
    ],
    "grammars": [
      {
        "language": "copl",
        "scopeName": "source.copl",
        "path": "./syntaxes/copl.json."
      }
    ],
  }
}
